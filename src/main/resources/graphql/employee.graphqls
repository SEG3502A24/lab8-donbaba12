
type Employee {
    id: ID!
    name: String!
    dateOfBirth: String!
    city: String!
    salary: Float!
    gender: String!
    email: String!
}

type Query {
    employees: [Employee]!
    employee(id: ID!): Employee
}

type Mutation {
    addEmployee(name: String!, dateOfBirth: String!, city: String!, salary: Float!, gender: String!, email: String!): Employee!
}

